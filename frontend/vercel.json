{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "silent": true
  },
  "env": {
    "VITE_BASE_URL": "@vite_base_url",
    "VITE_GOOGLE_CLIENT_ID": "@vite_google_client_id",
    "VITE_FACEBOOK_APP_ID": "@vite_facebook_app_id"
  }
}